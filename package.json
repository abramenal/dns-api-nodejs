{
  "name": "dns-api-nodejs",
  "version": "1.0.0",
  "description": "Drone navigation service",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "nodemon -e js -w ./src ./src/index.js",
